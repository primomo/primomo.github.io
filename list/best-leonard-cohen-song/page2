flowtype(document.body.querySelector('#gamemessage-container'), {
   minimum   : 0,
   maximum   : 600,
   minFont   : 1,
   maxFont   : 50,
   fontRatio : 14,
   lineRatio : 1.45
});	
</script>
</body>
</html>